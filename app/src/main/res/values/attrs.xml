        <attr name="thu" format="reference"/>
        <attr name="fri" format="reference"/>
        <attr name="sat" format="reference"/>

    </declare-styleable>

    <declare-styleable name="PageCircleIndicator">
        <attr name="ci_width" format="dimension"/>
        <attr name="ci_height" format="dimension"/>
        <attr name="ci_margin" format="dimension"/>
        <attr name="ci_animator" format="reference"/>
        <attr name="ci_animator_reverse" format="reference"/>
        <attr name="ci_drawable" format="reference"/>
        <attr name="ci_drawable_unselected" format="reference"/>
    </declare-styleable>
    <!-- XML attributes used by default_toppackage.xml -->
    <declare-styleable name="TopPackage">
        <attr name="topPackageName" format="string" />
        <attr name="topClassName" format="string" />
        <attr name="topOrder" format="integer" />
    </declare-styleable>

    <declare-styleable name="MovingDotView">
        <!--小点的总数-->
        <attr name="md_dot_count" format="integer"/>
        <!--中心大点的半径-->
        <attr name="md_center_dot_radius" format="dimension"/>
        <!--中心大点的资源文件-->
        <attr name="md_center_dot_back" format="reference"/>
        <!--小点的颜色-->
        <attr name="md_dot_color" format="color"/>
        <!--最大小点半径-->
        <attr name="md_dot_max_radius" format="dimension"/>
        <!--最小小点半径-->
        <attr name="md_dot_min_radius" format="dimension"/>
        <!--小圆点的最大速度(1~10)-->
        <attr name="md_dot_max_speed" format="integer"/>
        <!--小圆点的最小速度(1~10)-->
        <attr name="md_dot_min_speed" format="integer"/>
        <!--进度显示文字大小-->
        <attr name="md_text_size" format="dimension"/>
        <!--进度显示文字颜色-->
        <attr name="md_text_color" format="color"/>
        <!--加速按钮的文字颜色-->
        <attr name="md_btn_text_color" format="color"/>
        <!--动画持续的时间-->
        <attr name="md_animator_duration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SpringRecyclerView">
        <attr name="srv_enableSpringEffectWhenDrag" format="boolean"/>
        <attr name="srv_enableSpringEffectWhenFling" format="boolean"/>
        <attr name="srv_releaseBackAnimDuration" format="integer"/>
        <attr name="srv_flingBackAnimDuration" format="integer"/>
    </declare-styleable>

    <!-- 图片裁剪 -->
    <declare-styleable name="CropImageView">
        <attr name="cropGuidelines">
            <enum name="off" value="0"/>
            <enum name="onTouch" value="1"/>
            <enum name="on" value="2"/>
        </attr>
        <attr name="cropScaleType">
            <enum name="fitCenter" value="0"/>
            <enum name="center" value="1"/>
            <enum name="centerCrop" value="2"/>
            <enum name="centerInside" value="3"/>
        </attr>
