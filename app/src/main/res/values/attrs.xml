    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>
    <declare-styleable name="RoundImageView">
        <attr name="borderRadiusCustom" />
        <attr name="type" />
    </declare-styleable>

    <declare-styleable name="CustomClock">
        <attr name="dial" format="reference"/>
        <attr name="hand_hour" format="reference"/>
        <attr name="hand_minute" format="reference"/>
        <attr name="hand_second" format="reference"/>
        <attr name="center" format="reference"/>
        <attr name="battery" format="reference"/>
        <attr name="week" format="reference"/>
        <attr name="showweek" format="boolean"/>
        <attr name="showdate" format="boolean"/>
        <attr name="HeartRate" format="reference"/>
        <attr name="MyView_battery" format="reference"/>

        <attr name="hour" format="reference"/>
        <attr name="minute" format="reference"/>
        <attr name="second" format="reference"/>
        <attr name="date" format="reference"/>
        <attr name="step" format="reference"/>
        <attr name="wether" format="reference"/>
        <attr name="month" format="reference"/>
        <attr name="yuan" format="reference"/>


        <attr name="mSmall01" format="reference"/>
